{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "🏃‍♂️ Run fvm build",
			"command": "fvm",
			"args": [
				"flutter",
				"pub",
				"run",
				"build_runner",
				"build"
			]
		},
		{
			"label": "🏃‍♂️ Run fvm build with delete",
			"command": "fvm",
			"args": [
				"flutter",
				"pub",
				"run",
				"build_runner",
				"build",
				"--delete-conflicting-outputs"
			]
		},
	]
}